{
  "expo": {
    "name": "BTC Block Explorer",
    "description": "A safe, simple tool for observing Bitcoin addresses and their activity.",
    "slug": "btc-block-explorer",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.llapplications.btcblockexplorer"
    },
    "android" : {
      "package": "com.llapplications.btcblockexplorer",
      "permissions": ["CAMERA"]
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "githubUrl": "https://github.com/llazarus/btc_block_explorer",
    "primaryColor": "#ffffff"
  }
}